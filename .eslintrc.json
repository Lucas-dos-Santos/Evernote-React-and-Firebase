{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
			"eslint:recommended",
			"airbnb",
			"plugin:import/errors",
			"plugin:react/recommended",
			"plugin:jsx-a11y/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module",
				"ecmaFeatures": {
					"jsx": true
				}
    },
    "plugins": ["react", "import", "jsx-a11y"],
    "rules": {
			"react/prop-types": "off",
			"react/jsx-uses-react": "on",
			"react/react-in-jsx-scope": "off",
			"react/self-closing-comp": "warn"
    },
		"settings": {
			"react": {
				"version": "detect"
			}
		}
}
